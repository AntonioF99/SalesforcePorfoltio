<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ordering_of_End_Date_and_Start_Date</fullName>
    <active>true</active>
    <description>Sets the order of End Date after Star Date</description>
    <errorConditionFormula>AND(
  NOT(ISBLANK(End_Date__c)),
  End_Date__c &lt; Start_Date__c
)</errorConditionFormula>
    <errorMessage>End Date must be after Start Date</errorMessage>
</ValidationRule>
